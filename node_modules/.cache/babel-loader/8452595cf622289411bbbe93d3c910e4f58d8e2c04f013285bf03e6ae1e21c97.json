{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport DeliveryService from '@/services/DeliveryService';\nimport router from '../router';\nexport default {\n  data: () => ({\n    trips: null,\n    refreshAllTrips: false,\n    loadingOverlay: false,\n    editOverlay: false,\n    diableOverlay: true,\n    lochtlOverlay: false,\n    loadingMSG: \"Loading...\",\n    userPerms: \"\",\n    deliverys: []\n  }),\n  methods: {\n    async getAllDeliverys() {\n      await DeliveryService.getAllDeliverys().then(response => {\n        response.data.forEach(element => {\n          if (this.userPerms == \"3\") {\n            if (element.orderTakenBy == \"\" || element.orderTakenBy == null) {\n              this.deliverys.push(element);\n            }\n          } else {\n            this.deliverys.push(element);\n          }\n        });\n        this.loadingOverlay = false;\n      });\n    },\n    openDelivery(id) {\n      this.$store.commit('setviewDeliveryId', id);\n      router.push(\"/viewdelivery\");\n    },\n    editDelivery(id) {\n      console.log(id);\n      this.$store.commit('seteditDeliveryId', id);\n      router.push(\"/updatedelivery\");\n    }\n  },\n  beforeMount() {\n    this.loadingOverlay = true;\n    this.getAllDeliverys();\n  },\n  watch: {\n    refreshAllTrips: function () {\n      this.loadingOverlay = true;\n      this.deliverys = [];\n      this.userPerms = this.$store.state.permission;\n      this.getAllDeliverys();\n    }\n  }\n};","map":{"version":3,"names":["DeliveryService","router","data","trips","refreshAllTrips","loadingOverlay","editOverlay","diableOverlay","lochtlOverlay","loadingMSG","userPerms","deliverys","methods","getAllDeliverys","then","response","forEach","element","orderTakenBy","push","openDelivery","id","$store","commit","editDelivery","console","log","beforeMount","watch","state","permission"],"sources":["/Users/mahithareddyragi/Desktop/CourierSerivce/client/src/components/AllDeliverys.vue"],"sourcesContent":["<template>\n  <div :key=\"trips\">\n    <v-row class=\"text-right\"> \n      <v-col col=\"12\" mb=\"2\">\n        <v-btn\n          prepend-icon=\"mdi-account-circle\"\n          @click=\"refreshAllTrips = !refreshAllTrips\"\n        >\n          <template v-slot:prepend>\n            <v-icon color=\"primary\">mdi-refresh</v-icon>\n          </template>\n            Refresh\n          </v-btn>\n      </v-col>\n    </v-row>\n    <br/>\n    <v-card class=\"mx-auto px-10 py-8\" width=\"90%\" height=\"600\">\n          <br/>\n          <v-virtual-scroll :items=\"deliverys\" height=\"600\" item-height=\"50\">\n            <template v-slot:default=\"{ item }\">\n              <v-list-item>\n                <v-list-item-title>Delivery ID: {{ item.id }}</v-list-item-title>\n                <v-list-item-subtitle> {{ item.pickupCustomerName }} to {{ item.deliveryCustomerName }}</v-list-item-subtitle>\n                <v-list-item-subtitle>Pick Up: {{ item.pickupStreet }} - {{ item.pickupAvn }} &nbsp; Delivery: {{ item.deliveryStreet }} - {{ item.deliveryAvn }}</v-list-item-subtitle>\n                <template v-slot:append>\n                  <v-btn size=\"small\" variant=\"tonal\" @click=\"openDelivery(item.id)\">\n                    <v-icon color=\"orange-darken-4\" end> mdi-open-in-new  </v-icon>&nbsp;\n                    View\n                  </v-btn>\n                  &nbsp;\n                  <v-btn size=\"small\" variant=\"tonal\" @click=\"editDelivery(item.id)\" v-show=\"this.$store.state.permission != '3'\">\n                    <v-icon color=\"orange-darken-4\" end> mdi-pencil </v-icon>&nbsp;\n                    Edit\n                  </v-btn>\n                </template>\n              </v-list-item>\n              <v-divider></v-divider>\n            </template>\n          </v-virtual-scroll>\n        </v-card>\n   \n    <v-overlay\n      :model-value=\"loadingOverlay\"\n      class=\"align-center justify-center\"\n      :persistent=\"diableOverlay\"\n    >\n      <v-progress-circular\n        color=\"primary\"\n        indeterminate\n        size=\"64\"\n      ></v-progress-circular>\n      <h3>{{loadingMSG}}</h3>\n    </v-overlay>\n  </div>\n</template>\n\n<script>\nimport DeliveryService from '@/services/DeliveryService'\nimport router from '../router'\n\n  export default {\n    data: () => ({\n      trips: null,\n      refreshAllTrips: false,\n      loadingOverlay: false,\n      editOverlay: false,\n      diableOverlay:true,\n      lochtlOverlay: false,\n      loadingMSG: \"Loading...\",\n      userPerms: \"\",\n\n      deliverys: []\n    }),\n\n    methods: {\n      async getAllDeliverys(){\n        await DeliveryService.getAllDeliverys().then((response)=>{\n            response.data.forEach(element => {\n              if(this.userPerms == \"3\"){\n                if(element.orderTakenBy == \"\" || element.orderTakenBy == null){\n                  this.deliverys.push(element)\n                }\n              }else{\n                this.deliverys.push(element)\n              }\n            });\n            this.loadingOverlay = false\n        })\n      },\n      openDelivery(id){\n        this.$store.commit('setviewDeliveryId', id)\n        router.push(\"/viewdelivery\")\n      },\n      editDelivery(id){\n        console.log(id)\n        this.$store.commit('seteditDeliveryId', id)\n        router.push(\"/updatedelivery\")\n      }\n    },\n    beforeMount() {\n      this.loadingOverlay = true\n      this.getAllDeliverys()\n    },\n    watch: {\n      refreshAllTrips: function(){\n        this.loadingOverlay = true\n        this.deliverys = []\n        this.userPerms = this.$store.state.permission\n        this.getAllDeliverys()\n      }\n    }\n  }\n</script>\n"],"mappings":";AAyDA,OAAOA,eAAc,MAAO,4BAA2B;AACvD,OAAOC,MAAK,MAAO,WAAU;AAE3B,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,KAAK,EAAE,IAAI;IACXC,eAAe,EAAE,KAAK;IACtBC,cAAc,EAAE,KAAK;IACrBC,WAAW,EAAE,KAAK;IAClBC,aAAa,EAAC,IAAI;IAClBC,aAAa,EAAE,KAAK;IACpBC,UAAU,EAAE,YA