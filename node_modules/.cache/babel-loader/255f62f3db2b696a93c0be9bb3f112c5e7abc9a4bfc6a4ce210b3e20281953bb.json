{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CustomerService from '@/services/CustomerService';\nexport default {\n  data: () => ({\n    trips: null,\n    refreshAllTrips: false,\n    loadingOverlay: false,\n    editOverlay: false,\n    diableOverlay: true,\n    lochtlOverlay: false,\n    loadingMSG: \"Loading...\",\n    custName: \"\",\n    custStreet: \"\",\n    custAvenue: \"\",\n    customers: []\n  }),\n  methods: {\n    async getAllCustomers() {\n      try {\n        await CustomerService.getAllCustomers().then(response => {\n          console.log(response);\n          if (response.statusText == \"OK\") {\n            response.data.forEach(element => {\n              this.customers.push(element);\n            });\n          }\n          this.loadingOverlay = false;\n        });\n      } catch (err) {\n        console.log(err);\n        console.log(err.message);\n        this.showError(err.message);\n      }\n    },\n    async addCustomer() {\n      this.loadingOverlay = true;\n      try {\n        console.log(this.custName + this.custStreet + this.custAvenue);\n        await CustomerService.addCustomer({\n          name: this.custName,\n          street: this.custStreet,\n          avenue: this.custAvenue\n        }).then(response => {\n          console.log(response);\n          if (response.statusText == \"OK\") {\n            this.customers.push(response.data);\n            this.custName = \"\";\n            this.custStreet = \"\", this.custAvenue = \"\";\n            this.refreshAllTrips = !this.refreshAllTrips;\n          }\n          this.loadingOverlay = false;\n        });\n      } catch (err) {\n        console.log(err);\n        console.log(err.response.data.error);\n      }\n    },\n    async deleteCustomer(cust) {\n      console.log(cust);\n      this.loadingOverlay = true;\n      this.loadingMSG = \"Deleting...\";\n      var deleteCust = await CustomerService.deleteCustomer(cust.id).then(() => {\n        this.loadingOverlay = false;\n        this.refreshAllTrips = !this.refreshAllTrips;\n      });\n      console.log(deleteCust);\n    }\n  },\n  beforeMount() {\n    this.loadingOverlay = true;\n    this.getAllCustomers();\n  },\n  watch: {\n    refreshAllTrips: function () {\n      this.loadingOverlay = true;\n      this.customers = [];\n      this.getAllCustomers();\n    }\n  }\n};","map":{"version":3,"names":["CustomerService","data","trips","refreshAllTrips","loadingOverlay","editOverlay","diableOverlay","lochtlOverlay","loadingMSG","custName","custStreet","custAvenue","customers","methods","getAllCustomers","then","response","console","log","statusText","forEach","element","push","err","message","showError","addCustomer","name","street","avenue","error","deleteCustomer","cust","deleteCust","id","beforeMount","watch"],"sources":["/Users/mahithareddyragi/Desktop/CourierSerivce/client/src/components/AllCustomers.vue"],"sourcesContent":["<template>\n  <div :key=\"trips\">\n    <v-row class=\"text-right\"> \n      <v-col col=\"12\" mb=\"2\">\n        <v-btn\n          prepend-icon=\"mdi-account-circle\"\n          @click=\"refreshAllTrips = !refreshAllTrips\"\n        >\n          <template v-slot:prepend>\n            <v-icon color=\"primary\">mdi-refresh</v-icon>\n          </template>\n            Refresh\n          </v-btn>\n      </v-col>\n    </v-row>\n    <br/>\n    <v-card class=\"mx-auto px-10 py-8\" width=\"90%\" height=\"600\">\n          <v-row>\n            <v-col cols=\"8\">\n              <v-text-field\n                v-model=\"custName\"\n                class=\"mb-2\"\n                clearable\n                label=\"Customer Name\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"2\">\n              <v-select\n                v-model=\"custStreet\"\n                label=\"Select Street\"\n                :items=\"['A', 'B','C','D','E','F','G']\"\n              ></v-select>\n            </v-col>\n            <v-col cols=\"2\">\n              <v-select\n                v-model=\"custAvenue\"\n                label=\"Select Avenue\"\n                :items=\"['1', '2','3','4','5','6','7']\"\n              ></v-select>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"12\">\n              <v-btn \n                size=\"large\"\n                value=\"AddNewLocation\"\n                @click=\"addCustomer()\">\n                <v-icon>mdi-plus-circle-outline</v-icon>\n                Add Customer\n              </v-btn>\n            </v-col>\n          </v-row>\n          <br/>\n          <v-divider></v-divider>\n          <br/>\n          <v-virtual-scroll :items=\"customers\" height=\"500\" item-height=\"50\">\n            <template v-slot:default=\"{ item }\">\n              <v-list-item>\n                <v-list-item-title>Customer Name: {{ item.name }}</v-list-item-title>\n                <v-list-item-subtitle>Customer ID: {{ item.id }}</v-list-item-subtitle>\n                <v-list-item-subtitle>Street: {{ item.street }} - Avenue {{ item.avenue }}</v-list-item-subtitle>\n                <template v-slot:append>\n                  <v-btn size=\"small\" variant=\"tonal\" @click=\"deleteCustomer(item)\">\n                    <v-icon color=\"orange-darken-4\" end> mdi-delete-empty  </v-icon>&nbsp;\n                    Delete\n                  </v-btn>\n                  &nbsp;\n                </template>\n              </v-list-item>\n              <v-divider></v-divider>\n            </template>\n          </v-virtual-scroll>\n        </v-card>\n    \n    <v-overlay\n      :model-value=\"loadingOverlay\"\n      class=\"align-center justify-center\"\n      :persistent=\"diableOverlay\"\n    >\n      <v-progress-circular\n        color=\"primary\"\n        indeterminate\n        size=\"64\"\n      ></v-progress-circular>\n      <h3>{{loadingMSG}}</h3>\n    </v-overlay>\n  </div>\n</template>\n\n<script>\nimport CustomerService from '@/services/CustomerService'\n\n  export default {\n    data: () => ({\n      trips: null,\n      refreshAllTrips: false,\n      loadingOverlay: false,\n      editOverlay: false,\n      diableOverlay:true,\n\n      lochtlOverlay: false,\n      loadingMSG: \"Loading...\",\n      custName: \"\",\n      custStreet: \"\",\n      custAvenue: \"\",\n\n      customers: []\n    }),\n\n    methods: {\n      async getAllCustomers(){\n        try{\n          await CustomerService.getAllCustomers().then((response)=> {\n                console.log(response)\n                if(response.statusText == \"OK\"){\n                  response.data.forEach(element => {\n                    this.customers.push(element)\n                  });\n                }\n                this.loadingOverlay = false\n              })\n        }\n        catch(err){\n          console.log(err)\n          console.log(err.message)\n          this.showError(err.message)\n        }\n      },\n      async addCustomer(){\n        this.loadingOverlay = true\n        try{\n          console.log(this.custName + this.custStreet + this.custAvenue)\n          await CustomerService.addCustomer({\n              name: this.custName,\n              street: this.custStreet,\n              avenue: this.custAvenue\n          }).then((response)=> {\n                console.log(response)\n                if(response.statusText == \"OK\"){\n                  this.customers.push(response.data)\n                  this.custName = \"\"\n                  this.custStreet = \"\",\n                  this.custAvenue = \"\"\n                  this.refreshAllTrips = !this.refreshAllTrips\n                }\n                this.loadingOverlay = false\n              }\n          )\n        }\n        catch(err){\n          console.log(err)\n          console.log(err.response.data.error)\n        }\n      },\n      async deleteCustomer(cust){\n        console.log(cust)\n        this.loadingOverlay = true\n        this.loadingMSG = \"Deleting...\"\n        var deleteCust = await CustomerService.deleteCustomer(cust.id).then(()=> {\n          this.loadingOverlay = false\n          this.refreshAllTrips = !this.refreshAllTrips\n        })\n        console.log(deleteCust)\n      }\n    },\n    beforeMount() {\n      this.loadingOverlay = true\n      this.getAllCustomers()\n    },\n    watch: {\n      refreshAllTrips: function(){\n        this.loadingOverlay = true\n        this.customers = []\n        this.getAllCustomers()\n      }\n    }\n  }\n</script>\n"],"mappings":";AA0FA,OAAOA,eAAc,MAAO,4BAA2B;AAErD,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,KAAK,EAAE,IAAI;IACXC,eAAe,EAAE,KAAK;IACtBC,cAAc,EAAE,KAAK;IACrBC,WAAW,EAAE,KAAK;IAClBC,aAAa,EAAC,IAAI;IAElBC,aAAa,EAAE,KAAK;IACpBC,UAAU,EAAE,YAAY;IACxBC,QAAQ,EAAE,EAAE;IACZC,UAAU,EAAE,EAAE;IACdC,UAAU,EAAE,EAAE;IAEdC,SAAS,EAAE;EACb,CAAC,CAAC;EAEFC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAE;MACrB,IAAG;QACD,MAAMd,eAAe,CAACc,eAAe,CAAC,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAI;UACpDC,OAAO,CAACC,GAAG,CAACF,QAAQ;UACpB,IAAGA,QAAQ,CAACG,UAAS,IAAK,IAAI,EAAC;YAC7BH,QAAQ,CAACf,IAAI,CAACmB,OAAO,CAACC,OAAM,IAAK;cAC/B,IAAI,CAACT,SAAS,CAACU,IAAI,CAACD,OAAO;YAC7B,CAAC,CAAC;UACJ;UACA,IAAI,CAACjB,cAAa,GAAI,KAAI;QAC5B,CAAC;MACP,EACA,OAAMmB,GAAG,EAAC;QACRN,OAAO,CAACC,GAAG,CAACK,GAAG;QACfN,OAAO,CAACC,GAAG,CAACK,GAAG,CAACC,OAAO;QACvB,IAAI,CAACC,SAAS,CAACF,GAAG,CAACC,OAAO;MAC5B;IACF,CAAC;IACD,MAAME,WAAWA,CAAA,EAAE;MACjB,IAAI,CAACtB,cAAa,GAAI,IAAG;MACzB,IAAG;QACDa,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,QAAO,GAAI,IAAI,CAACC,UAAS,GAAI,IAAI,CAACC,UAAU;QAC7D,MAAMX,eAAe,CAAC0B,WAAW,CAAC;UAC9BC,IAAI,EAAE,IAAI,CAAClB,QAAQ;UACnBmB,MAAM,EAAE,IAAI,CAAClB,UAAU;UACvBmB,MAAM,EAAE,IAAI,CAAClB;QACjB,CAAC,CAAC,CAACI,IAAI,CAAEC,QAAQ,IAAI;UACfC,OAAO,CAACC,GAAG,CAACF,QAAQ;UACpB,IAAGA,QAAQ,CAACG,UAAS,IAAK,IAAI,EAAC;YAC7B,IAAI,CAACP,SAAS,CAACU,IAAI,CAACN,QAAQ,CAACf,IAAI;YACjC,IAAI,CAACQ,QAAO,GAAI,EAAC;YACjB,IAAI,CAACC,UAAS,GAAI,EAAE,EACpB,IAAI,CAACC,UAAS,GAAI,EAAC;YACnB,IAAI,CAACR,eAAc,GAAI,CAAC,IAAI,CAACA,eAAc;UAC7C;UACA,IAAI,CAACC,cAAa,GAAI,KAAI;