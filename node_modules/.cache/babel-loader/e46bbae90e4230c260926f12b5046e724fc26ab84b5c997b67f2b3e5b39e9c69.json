{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ItinerarysService from '@/services/ItinerarysService';\nimport DeliveryService from '@/services/DeliveryService';\nexport default {\n  data: () => ({\n    trips: null,\n    refreshAllTrips: false,\n    loadingOverlay: false,\n    editOverlay: false,\n    diableOverlay: true,\n    createHtlIMG: \"\",\n    createHtl: \"\",\n    editHtlIMG: \"\",\n    editHtl: \"\",\n    editHtlID: \"\",\n    allHtl: [],\n    addHtlAry: [],\n    showHtl: [],\n    lochtlOverlay: false,\n    loadingMSG: \"Loading...\",\n    deliverys: []\n  }),\n  methods: {\n    async getAllDeliverys() {\n      await DeliveryService.getAllDeliverys().then(response => {\n        response.data.forEach(element => {\n          this.deliverys.push(element);\n        });\n        this.loadingOverlay = false;\n      });\n    },\n    async createHotel() {\n      if (this.createHtl != \"\" && this.createHtlIMG != null) {\n        this.loadingOverlay = true;\n        await ItinerarysService.addHotel({\n          HotelName: this.createHtl,\n          ImageURL: this.createHtlIMG\n        }).then(response => {\n          console.log(\"create Location: \" + response);\n          this.allHtl.push({\n            \"id\": response.data.id,\n            \"HotelName\": response.data.HotelName,\n            \"ImageURL\": response.data.ImageURL\n          });\n          this.addHtlAry.push({\n            \"HotelName\": this.createHtl,\n            \"ImageURL\": this.createHtlIMG\n          });\n          this.createHtl = \"\";\n          this.createHtlIMG = \"\";\n          this.loadingOverlay = false;\n        });\n      } else {\n        this.showSnackles(\"Please Fill hotel name and Hoptel image URL to continue.\");\n      }\n    },\n    editHotel(hot) {\n      console.log(hot);\n      this.editHtl = hot.HotelName;\n      this.editHtlIMG = hot.ImageURL;\n      this.editHtlID = hot.id;\n      this.editOverlay = !this.editOverlay;\n    },\n    async deleteHotel(hot) {\n      console.log(hot);\n      this.loadingOverlay = true;\n      this.loadingMSG = \"Deleting...\";\n      var deleteHot = await ItinerarysService.deleteHotel(hot.id).then(() => {\n        this.loadingOverlay = false;\n        this.refreshAllTrips = !this.refreshAllTrips;\n      });\n      console.log(deleteHot);\n    },\n    async updateHtl() {\n      console.log(this.editHtlID);\n      this.loadingOverlay = true;\n      this.loadingMSG = \"Updating...\";\n      var uptHtl = await ItinerarysService.updateHotel(this.editHtlID, {\n        HotelName: this.editHtl,\n        ImageURL: this.editHtlIMG\n      }).then(() => {\n        this.editOverlay = !this.editOverlay;\n        this.loadingOverlay = false;\n        this.refreshAllTrips = !this.refreshAllTrips;\n      });\n      console.log(\"updated: \" + uptHtl);\n    }\n  },\n  beforeMount() {\n    this.loadingOverlay = true;\n    this.getAllDeliverys();\n  },\n  watch: {\n    refreshAllTrips: function () {\n      this.loadingOverlay = true;\n      this.deliverys = [];\n      this.getAllDeliverys();\n    }\n  }\n};","map":{"version":3,"names":["ItinerarysService","DeliveryService","data","trips","refreshAllTrips","loadingOverlay","editOverlay","diableOverlay","createHtlIMG","createHtl","editHtlIMG","editHtl","editHtlID","allHtl","addHtlAry","showHtl","lochtlOverlay","loadingMSG","deliverys","methods","getAllDeliverys","then","response","forEach","element","push","createHotel","addHotel","HotelName","ImageURL","console","log","id","showSnackles","editHotel","hot","deleteHotel","deleteHot","updateHtl","uptHtl","updateHotel","beforeMount","watch"],"sources":["/Users/meenusnehithirava/Desktop/acme/client/src/components/AllDeliverys.vue"],"sourcesContent":["<template>\n  <div :key=\"trips\">\n    <v-row class=\"text-right\"> \n      <v-col col=\"12\" mb=\"2\">\n        <v-btn\n          prepend-icon=\"mdi-account-circle\"\n          @click=\"refreshAllTrips = !refreshAllTrips\"\n        >\n          <template v-slot:prepend>\n            <v-icon color=\"primary\">mdi-refresh</v-icon>\n          </template>\n            Refresh\n          </v-btn>\n      </v-col>\n    </v-row>\n    <br/>\n    <v-card class=\"mx-auto px-10 py-8\" width=\"90%\" height=\"600\">\n          <br/>\n          <v-virtual-scroll :items=\"deliverys\" height=\"600\" item-height=\"50\">\n            <template v-slot:default=\"{ item }\">\n              <v-list-item>\n                <v-list-item-title>Delivery ID: {{ item.id }}</v-list-item-title>\n                <v-list-item-subtitle> {{ item.pickupCustomerName }} to {{ item.deliveryCustomerName }}</v-list-item-subtitle>\n                <v-list-item-subtitle>Pick Up: {{ item.pickupStreet }} - {{ item.pickupAvn }} &nbsp; Delivery: {{ item.deliveryStreet }} - {{ item.deliveryAvn }}</v-list-item-subtitle>\n                <template v-slot:append>\n                  <v-btn size=\"small\" variant=\"tonal\">\n                    <v-icon color=\"orange-darken-4\" end> mdi-pencil  </v-icon>&nbsp;\n                    View\n                  </v-btn>\n                  &nbsp;\n                  <v-btn size=\"small\" variant=\"tonal\">\n                    <v-icon color=\"orange-darken-4\" end> mdi-delete-empty </v-icon>&nbsp;\n                    Edit\n                  </v-btn>\n                </template>\n              </v-list-item>\n              <v-divider></v-divider>\n            </template>\n          </v-virtual-scroll>\n        </v-card>\n   \n    <v-overlay\n      :model-value=\"loadingOverlay\"\n      class=\"align-center justify-center\"\n      :persistent=\"diableOverlay\"\n    >\n      <v-progress-circular\n        color=\"primary\"\n        indeterminate\n        size=\"64\"\n      ></v-progress-circular>\n      <h3>{{loadingMSG}}</h3>\n    </v-overlay>\n  </div>\n</template>\n\n<script>\nimport ItinerarysService from '@/services/ItinerarysService'\nimport DeliveryService from '@/services/DeliveryService'\n\n  export default {\n    data: () => ({\n      trips: null,\n      refreshAllTrips: false,\n      loadingOverlay: false,\n      editOverlay: false,\n      diableOverlay:true,\n\n      createHtlIMG: \"\",\n      createHtl: \"\",\n      editHtlIMG: \"\",\n      editHtl: \"\",\n      editHtlID: \"\",\n      allHtl: [],\n      addHtlAry: [],\n      showHtl: [],\n      lochtlOverlay: false,\n      loadingMSG: \"Loading...\",\n\n      deliverys: []\n    }),\n\n    methods: {\n      async getAllDeliverys(){\n        await DeliveryService.getAllDeliverys().then((response)=>{\n            response.data.forEach(element => {\n              this.deliverys.push(element)\n            });\n            this.loadingOverlay = false\n        })\n      },\n      async createHotel(){\n        if(this.createHtl != \"\" && this.createHtlIMG != null){\n        this.loadingOverlay = true\n          await ItinerarysService.addHotel({\n            HotelName: this.createHtl,\n            ImageURL: this.createHtlIMG\n          }).then((response)=>{\n            console.log(\"create Location: \" + response)\n            this.allHtl.push({\n              \"id\" : response.data.id,\n              \"HotelName\" : response.data.HotelName,\n              \"ImageURL\" : response.data.ImageURL\n            })\n            \n            this.addHtlAry.push({\n              \"HotelName\" : this.createHtl,\n              \"ImageURL\" : this.createHtlIMG\n            })\n            this.createHtl = \"\"\n            this.createHtlIMG = \"\"\n            \n            this.loadingOverlay = false\n          })\n        }\n          else{\n            this.showSnackles(\"Please Fill hotel name and Hoptel image URL to continue.\")\n          }\n      },\n      editHotel(hot){\n        console.log(hot)\n        this.editHtl = hot.HotelName\n        this.editHtlIMG = hot.ImageURL\n        this.editHtlID = hot.id\n        this.editOverlay = !this.editOverlay\n      },\n      async deleteHotel(hot){\n        console.log(hot)\n        this.loadingOverlay = true\n        this.loadingMSG = \"Deleting...\"\n        var deleteHot = await ItinerarysService.deleteHotel(hot.id).then(()=> {\n          this.loadingOverlay = false\n          this.refreshAllTrips = !this.refreshAllTrips\n        })\n        console.log(deleteHot)\n      },\n      async updateHtl(){\n        console.log(this.editHtlID)\n        this.loadingOverlay = true\n        this.loadingMSG = \"Updating...\"\n        var uptHtl = await ItinerarysService.updateHotel(this.editHtlID, {\n          HotelName: this.editHtl,\n          ImageURL: this.editHtlIMG\n        }).then(() => {\n          this.editOverlay = !this.editOverlay\n          this.loadingOverlay = false\n          this.refreshAllTrips = !this.refreshAllTrips\n        })\n        console.log(\"updated: \"+ uptHtl)\n      }\n    },\n    beforeMount() {\n      this.loadingOverlay = true\n      this.getAllDeliverys()\n    },\n    watch: {\n      refreshAllTrips: function(){\n        this.loadingOverlay = true\n        this.deliverys = []\n        this.getAllDeliverys()\n      }\n    }\n  }\n</script>\n"],"mappings":";AAyDA,OAAOA,iBAAgB,MAAO,8BAA6B;AAC3D,OAAOC,eAAc,MAAO,4BAA2B;AAErD,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,KAAK,EAAE,IAAI;IACXC,eAAe,EAAE,KAAK;IACtBC,cAAc,EAAE,KAAK;IACrBC,WAAW,EAAE,KAAK;IAClBC,aAAa,EAAC,IAAI;IAElBC,YAAY,EAAE,EAAE;IAChBC,SAAS,EAAE,EAAE;IACbC,UAAU,EAAE,EAAE;IACdC,OAAO,EAAE,EAAE;IACXC,SAAS,EAAE,EAAE;IACbC,MAAM,EAAE,EAAE;IACVC,SAAS,EAAE,EAAE;IACbC,OAAO,EAAE,EAAE;IACXC,aAAa,EAAE,KAAK;IACpBC,UAAU,EAAE,YAAY;IAExBC,SAAS,EAAE;EACb,CAAC,CAAC;EAEFC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAE;MACrB,MAAMnB,eAAe,CAACmB,eAAe,CAAC,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAG;QACrDA,QAAQ,CAACpB,IAAI,CAACqB,OAAO,CAACC,OAAM,IAAK;UAC/B,IAAI,CAACN,SAAS,CAACO,IAAI,CAACD,OAAO;QAC7B,CAAC,CAAC;QACF,IAAI,CAACnB,cAAa,GAAI,KAAI;MAC9B,CAAC;IACH,CAAC;IACD,MAAMqB,WAAWA,CAAA,EAAE;MACjB,IAAG,IAAI,CAACjB,SAAQ,IAAK,EAAC,IAAK,IAAI,CAACD,YAAW,IAAK,IAAI,EAAC;QACrD,IAAI,CAACH,cAAa,GAAI,IAAG;QACvB,MAAML,iBAAiB,CAAC