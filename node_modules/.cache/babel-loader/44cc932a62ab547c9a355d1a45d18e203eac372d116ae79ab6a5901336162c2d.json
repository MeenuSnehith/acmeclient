{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport DeliveryService from '@/services/DeliveryService';\nimport router from '../router';\nexport default {\n  data: () => ({\n    trips: null,\n    refreshAllTrips: false,\n    loadingOverlay: false,\n    editOverlay: false,\n    diableOverlay: true,\n    lochtlOverlay: false,\n    loadingMSG: \"Loading...\",\n    username: \"\",\n    deliverys: [],\n    deliveryCount: 0,\n    bonusCount: 0,\n    completedCount: 0\n  }),\n  methods: {\n    async getAllDeliverys() {\n      await DeliveryService.getlDeliverysByUser(this.username).then(response => {\n        response.data.forEach(element => {\n          this.deliverys.push(element);\n          this.deliveryCount = this.deliveryCount + 1;\n          if (element.isBonus == 'yes') {\n            this.bonusCount = this.bonusCount + 1;\n          }\n          if (element.status == 4) {\n            this.completedCount = this.completedCount + 1;\n          }\n        });\n        this.loadingOverlay = false;\n      });\n    },\n    openDelivery(id) {\n      this.$store.commit('setviewDeliveryId', id);\n      router.push(\"/viewdelivery\");\n    }\n  },\n  beforeMount() {\n    this.loadingOverlay = true;\n    this.username = this.$store.state.username;\n    this.getAllDeliverys();\n  },\n  watch: {\n    refreshAllTrips: function () {\n      this.loadingOverlay = true;\n      this.deliverys = [];\n      this.getAllDeliverys();\n    }\n  }\n};","map":{"version":3,"names":["DeliveryService","router","data","trips","refreshAllTrips","loadingOverlay","editOverlay","diableOverlay","lochtlOverlay","loadingMSG","username","deliverys","deliveryCount","bonusCount","completedCount","methods","getAllDeliverys","getlDeliverysByUser","then","response","forEach","element","push","isBonus","status","openDelivery","id","$store","commit","beforeMount","state","watch"],"sources":["/Users/mahithareddyragi/Desktop/CourierSerivce/client/src/components/MyProfile.vue"],"sourcesContent":["<template>\n  <div :key=\"trips\">\n    <v-row class=\"text-right\"> \n      <v-col col=\"12\" mb=\"2\">\n        <v-btn\n          prepend-icon=\"mdi-account-circle\"\n          @click=\"refreshAllTrips = !refreshAllTrips\"\n        >\n          <template v-slot:prepend>\n            <v-icon color=\"primary\">mdi-refresh</v-icon>\n          </template>\n            Refresh\n          </v-btn>\n      </v-col>\n    </v-row>\n    <br/>\n    <v-card class=\"mx-auto px-10 py-8\" width=\"90%\" height=\"200\">\n      <v-row>\n        <v-col>\n          <p><b>User Name: </b> {{ username }}</p>\n          <p><b>Number of Deliveries:</b> {{ deliveryCount }}</p>\n          <p><b>Number of Completed Deliveries:</b> {{ completedCount }}</p>\n          <p><b>Number of Bonus from Delivery:</b> {{ bonusCount }}</p>\n        </v-col>\n      </v-row>\n    </v-card>\n    <v-card class=\"mx-auto px-10 py-8\" width=\"90%\" height=\"600\">\n          <br/>\n          <h2>Deliveries</h2>\n          <v-virtual-scroll :items=\"deliverys\" height=\"600\" item-height=\"50\">\n            <template v-slot:default=\"{ item }\">\n              <v-list-item>\n                <v-list-item-title>Delivery ID: {{ item.id }}</v-list-item-title>\n                <v-list-item-subtitle> {{ item.pickupCustomerName }} to {{ item.deliveryCustomerName }}</v-list-item-subtitle>\n                <v-list-item-subtitle>Pick Up: {{ item.pickupStreet }} - {{ item.pickupAvn }} &nbsp; Delivery: {{ item.deliveryStreet }} - {{ item.deliveryAvn }}</v-list-item-subtitle>\n                <template v-slot:append>\n                  <v-icon color=\"yellow-darken-4\" v-show=\"item.isBonus == 'yes'\"> mdi-star </v-icon>&nbsp;&nbsp;\n                  <v-icon color=\"green-darken-4\" v-show=\"item.status == 4\"> mdi-check-circle </v-icon>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                  <v-btn size=\"small\" variant=\"tonal\" @click=\"openDelivery(item.id)\">\n                    <v-icon color=\"orange-darken-4\" end> mdi-open-in-new </v-icon>&nbsp;\n                    View\n                  </v-btn>\n                </template>\n              </v-list-item>\n              <v-divider></v-divider>\n            </template>\n          </v-virtual-scroll>\n        </v-card>\n   \n    <v-overlay\n      :model-value=\"loadingOverlay\"\n      class=\"align-center justify-center\"\n      :persistent=\"diableOverlay\"\n    >\n      <v-progress-circular\n        color=\"primary\"\n        indeterminate\n        size=\"64\"\n      ></v-progress-circular>\n      <h3>{{loadingMSG}}</h3>\n    </v-overlay>\n  </div>\n</template>\n\n<script>\nimport DeliveryService from '@/services/DeliveryService'\nimport router from '../router'\n\n  export default {\n    data: () => ({\n      trips: null,\n      refreshAllTrips: false,\n      loadingOverlay: false,\n      editOverlay: false,\n      diableOverlay:true,\n\n      lochtlOverlay: false,\n      loadingMSG: \"Loading...\",\n      username: \"\",\n\n      deliverys: [],\n      deliveryCount: 0,\n      bonusCount: 0,\n      completedCount: 0\n    }),\n\n    methods: {\n      async getAllDeliverys(){\n        await DeliveryService.getlDeliverysByUser(this.username).then((response)=>{\n            response.data.forEach(element => {\n              this.deliverys.push(element)\n              this.deliveryCount = this.deliveryCount + 1\n              if(element.isBonus == 'yes'){\n                this.bonusCount = this.bonusCount + 1\n              }\n              if(element.status == 4){\n                this.completedCount = this.completedCount + 1\n              }\n            });\n            this.loadingOverlay = false\n        })\n      },\n      openDelivery(id){\n        this.$store.commit('setviewDeliveryId', id)\n        router.push(\"/viewdelivery\")\n      }\n    },\n    beforeMount() {\n      this.loadingOverlay = true\n      this.username = this.$store.state.username\n      this.getAllDeliverys()\n    },\n    watch: {\n      refreshAllTrips: function(){\n        this.loadingOverlay = true\n        this.deliverys = []\n        this.getAllDeliverys()\n      }\n    }\n  }\n</script>\n"],"mappings":";AAiEA,OAAOA,eAAc,MAAO,4BAA2B;AACvD,OAAOC,MAAK,MAAO,WAAU;AAE3B,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,KAAK,EAAE,IAAI;IACXC,eAAe,EAAE,KAAK;IACtBC,cAAc,EAAE,KAAK;IACrBC,WAAW,EAAE,KAAK;IAClBC,aAAa,EAAC,IAAI;IAElBC,aAAa,EAAE,KAAK;IACpBC,UAAU,EAAE,YAAY;IACxBC,QAAQ,EAAE,EAAE;IAEZC,SAAS,EAAE,EAAE;IACbC,aAAa,EAAE,CAAC;IAChBC,UAAU,EAAE,CAAC;IACbC,cAAc,EAAE;EAClB,CAAC,CAAC;EAEFC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAE;MACrB,MAAMhB,eAAe,CAACiB,mBAAmB,CAAC,IAAI,CAACP,QAAQ,CAAC,CAACQ,IAAI,CAAEC,QAAQ,IAAG;QACtEA,QAAQ,CAACjB,IAAI,CAACkB,OAAO,CAACC,OAAM,IAAK;UAC/B,IAAI,CAACV,SAAS,CAACW,IAAI,CAACD,OAAO;UAC3B,IAAI,CAACT,aAAY,GAAI,IAAI,CAACA,aAAY,GAAI;UAC1C,IAAGS,OAAO,CAACE,OAAM,IAAK,KAAK,EAAC;YAC1B,IAAI,CAACV,UAAS,GAAI,IAAI,CAACA,UAAS,GAAI;UACtC;UACA,IAAGQ,OAAO,CAACG,MAAK,IAAK,CAAC,EAAC;YACrB,IAAI,CAACV,cAAa,GAAI,IAAI,CAACA,cAAa,GAAI;UAC9C;QACF,CAAC,CAAC;QACF,IAAI,CAACT,cAAa,GAAI,KAAI;MAC9B,CAAC;IACH,CAAC;IACDoB,YAAYA,CAACC,EAAE,EAAC;MACd,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,mBAAmB,EAAEF,EAAE;MAC1CzB,MAAM,CAACqB,IAAI,CAAC,eAAe;IAC7B;EACF,CAAC;EACDO,WAAWA,CAAA,EAAG;IACZ,IAAI,CAACxB,cAAa,GAAI,IAAG;IACzB,IAAI,CAACK,QAAO,GAAI,IAAI,CAACiB,MAAM,CAACG,KAAK,CAACpB,QAAO;IACzC,IAAI,CAACM,eAAe,CAAC;EACvB,CAAC;EACDe,KAAK,EAAE;IACL3B,eAAe,EAAE,SAAAA,CAAA,EAAU;MACzB,IAAI,CAACC,cAAa,GAAI,IAAG;MACzB,IAAI,CAACM,SAAQ,GAAI,EAAC;MAClB,IAAI,CAACK,eAAe,CAAC;IACvB;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}